<head>
  <link rel="stylesheet" href="/css/sidebar.css">
</head>
<nav class="sidebar_navtag">
  <div class="sidebar_options">
    {{#each sidebarMenu }}
    <h4>{{this.title}}</h4>
    <ul class="subsidebar_options">
      {{#each this.sub }}
      <li><a href={{this.link}}>{{this.title}}</a></li>
      {{/each}}
    </ul>
    {{/each}}
  </div>
</nav>